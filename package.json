{
  "name": "gen3.skylerdong.com",
  "version": "0.1.0",
  "description": "Working repository for my 3rd-generation personal website at skylerdong.com",
  "repository": "https://github.com/dongskyler/gen3.skylerdong.com.git",
  "author": "Skyler Dong <skylerdong@protonmail.com>",
  "private": true,
  "license": "BSD-3-Clause",
  "scripts": {
    "format": "prettier './**/*.{js,jsx,ts,tsx,json,html,md,css,scss}' --write",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' --cache --fix",
    "clean": "rimraf build",
    "build": "yarn clean && react-scripts build",
    "test": "react-scripts test",
    "start": "NODE_ENV=development && yarn --frozen-lockfile && react-scripts start",
    "serve": "serve -s build",
    "production": "NODE_ENV=production && yarn build && yarn serve",
    "eject": "react-scripts eject",
    "refresh": "ramraf node_modules && yarn --frozen-lockfile"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,html,md,css,scss}": [
      "prettier --write"
    ],
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "jquery": "^3.5.1",
    "prop-types": "^15.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-ga": "^3.3.0",
    "react-router": "^4.3.0",
    "react-router-dom": "^5.2.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^3.4.3",
    "sass": "^1.32.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^16.7.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "~2.34.0",
    "@typescript-eslint/parser": "~2.34.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "eslint": "~6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "~2.5.1",
    "husky": "^4.3.6",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "typescript": "^4.1.2"
  }
}
